{
    "version": 2,
    "builds": [
      {
        "src": "bot.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/webhook",
        "dest": "/api/webhook"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
  }
  