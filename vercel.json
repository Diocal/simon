{
    "version": 2,
    "builds": [
      {
        "src": "bot.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "public/style.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/script.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/webhook",
        "dest": "/api/bot.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }
  